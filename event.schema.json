{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["venue", "date", "title", "startTime"],
    "properties": {
      "venue": { "type": "string", "minLength": 1 },
      "date": {
        "type": "string",
        "format": "date",
        "pattern": "^20[2-9][0-9]-[0-1][0-9]-[0-3][0-9]$"
      },
      "title": { "type": "string", "minLength": 1 },
      "artists": {
        "type": "array",
        "items": { "type": "string" },
        "minItems": 1
      },
      "startTime": {
        "type": "string",
        "pattern": "^([01]\\d|2[0-3]):[0-5]\\d$"
      },
      "url": {
        "type": "string",
        "format": "uri"
      }
    },
    "additionalProperties": false
  }
}
